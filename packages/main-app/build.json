{
  "vite": {
    // "build": {
    //   "sourcemap": true
    // },
    "optimizeDeps": {
      "disabled": true,
      "include": [
        "universal-env"
      ]
    //   "needsInterop": [
    //     "universal-env"
    //   ],
    //   "exclude": [
    //     "create-app-shared",
    //     "react-app-renderer",
    //     "@ice/runtime"
    //   ]
    }
  },
  "hash": true,
  "outputAssetsPath": {
    "js": "js",
    "css": "css"
  },
  "publicPath": "/",
  "plugins": [
    [
      "build-plugin-icestark",
      {
        "type": "framework"
      }
    ],
    [
      "build-plugin-fusion",
      {
        "disableModularImport": true,
        "themePackage": "@alifd/theme-design-pro",
        "themeConfig": {
          "css-prefix": "next-icestark-"
        }
      }
    ],
    [
      "build-plugin-ignore-style",
      {
        "libraryName": "@alifd/next"
      }
    ]
  ],
  "modeConfig": {
    "development": {},
    "production": {}
  }
}