{
  "hash": true,
  "outputAssetsPath": {
    "js": "js",
    "css": "css"
  },
  "publicPath": "/rc",
  "vite": {
    "base": "/",
    "css": {
      "postcss": "./postcss.config.js"
    }
  },
  "plugins": [
    ["build-plugin-icestark", {
      "type": "child"
    }],
    ["build-plugin-moment-locales", {
      "locales": ["zh-cn"]
    }],
    ["build-plugin-ignore-style", {
      "libraryName": "@alifd/next"
    }]
  ]
}
