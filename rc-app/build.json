{
  "hash": true,
  "outputAssetsPath": {
    "js": "js",
    "css": "css"
  },
  "publicPath": "./",
  "vite": {
    "base": "/sub-app/rc"
  },
  "plugins": [
    ["build-plugin-icestark", {
      "type": "child"
    }],
    ["build-plugin-moment-locales", {
      "locales": ["zh-cn"]
    }],
    ["build-plugin-ignore-style", {
      "libraryName": "@alifd/next"
    }]
  ]
}
